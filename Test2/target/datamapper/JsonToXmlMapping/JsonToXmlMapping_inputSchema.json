{
  "$schema": "http://wso2.org/json-schema/wso2-data-mapper-v5.0.0/schema#",
  "type": "object",
  "title": "root",
  "properties": {
    "transactionId": {
      "type": "string"
    },
    "customerId": {
      "type": "string"
    },
    "transactionDate": {
      "type": "string"
    },
    "amount": {
      "type": "number"
    },
    "currency": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "paymentMethods": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            },
            "accountEmail": {
              "type": "string"
            }
          }
        }
      ]
    },
    "merchant": {
      "type": "object",
      "properties": {
        "merchantId": {
          "type": "string"
        },
        "merchantName": {
          "type": "string"
        }
      }
    }
  },
  "inputType": "JSON"
}